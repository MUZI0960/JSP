<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.or.ddit.commons.dao.OthersDAO">
	<select id="selectLprodList" resultType="hashMap">
		SELECT LPROD_GU "lprodGu", LPROD_NM "lprodNm"
		FROM LPROD
	</select>
	<select id="selectBuyerList" resultType="BuyerVO">
		SELECT BUYER_ID, BUYER_NAME, BUYER_LGU
		FROM BUYER
	</select>
</mapper>